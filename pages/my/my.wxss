/* pages/three/three.wxss */
.pageView{
  width: 100%;
  height: 100vh;
  /* display: flex; */
  background-color: #f4f6f8;
}
.pageView .topView{
  width: 100%;
  height: 26vh;
  background-color:#6092ec;
  display: flex;
  flex-direction: row;
  /* display: flex;
  align-items: center;
  justify-content: center; */
  border-bottom-left-radius: 5%;
  border-bottom-right-radius: 5%;
}

.pageView .topView .leftBox {
   width: 30%;
   height: 25vh;
   /* border: 1px solid #000; */
  display: flex;
  align-items: center;
  justify-content: center;
}

.pageView .topView .rightBox {
  width: 70%;
  height: 25vh;
  /* border: 1px solid #000; */
 display: flex;
 align-items: center;
 color: white;
 font-size: 28rpx;
 /* justify-content: center; */
 position: relative;
}

.pageView .topView .login-rightbox {
  width: 70%;
  height: 25vh;
  /* border: 1px solid #000; */
 display: flex;
 align-items: center;
 color: white;
 font-size: 28rpx;
 /* justify-content: center; */
 position: relative;
}

.pageView .topView .rightBox image {
  height: 30rpx;
  width: 30rpx;
  position: absolute;
  right: 50rpx;
  /* border: 1px solid white; */
  z-index: 999;
}

.pageView .topView .leftBox image {
  height: 130rpx;
  width: 130rpx;
  border-radius: 50%;
  /* border: 1px solid white; */
}


.pageView .contentView {
  width: 100%;
  height: 600rpx;
  display: flex;
  /* border: 1px solid #000; */
  flex-direction: column;
  align-items:center;
  justify-content:center;
  position: relative;
  top: -90rpx;
}

.pageView .contentView .calendarBox {
  height: 250rpx;
  width: 90%;
  background-color: #ffffff;
  /* border: 1px solid #000; */
  margin-bottom: 30rpx;
  border-radius: 10px;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}

.pageView .contentView .calendarBox image {
  margin-top: 20rpx;
  height: 180rpx;
  width: 90%;
}

.pageView .contentView .calendarBox .title {
  font-size: 20rpx;
  position: absolute;
  left: 10rpx;
  top: 5rpx;
  z-index: 98;
  color: rgba(0, 0, 0, 0.651);
}

.pageView .contentView .lockBox {
  background-color: white;
  height: 250rpx;
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 99;
  opacity: 80%;
  border-radius: 10px;
  display: flex;
  justify-content: center;
  align-items: center;
}

.pageView .contentView .lockBox image {
  width: 60rpx;
  height: 60rpx;
  opacity: 30%;
}

.pageView .contentView .tabsBox {
  width: 90%;
  align-content: center;
  justify-content: center;
}

.pageView .contentView .tabsBox .tabBox {
  width: 100%;
  height: 60rpx;
  /* border: 1px solid #000; */
  display: flex;
  flex-direction: row;
  align-content: center;
  /* justify-content: center; */
  position: relative;
  background-color: #ffffff;
  border-bottom: 1px solid blanchedalmond;
  padding: 10rpx 0rpx 10rpx 10rpx;
}

.pageView .contentView .tabsBox .tabBox:last-child{
  border-bottom-color: #ffffff;
}

.pageView .contentView .tabsBox .tabBox image:nth-child(1) {
  height: 50rpx;
  width: 50rpx;
  margin: auto 0px;
}

.pageView .contentView .tabsBox .tabBox text {
  position: relative;
  top: 15rpx;
  left: 25rpx;
  font-size: 26rpx;
  text-align: center;
}

.pageView .contentView .tabsBox .tabBox image:last-child {
  height: 40rpx;
  width: 40rpx;
  position: absolute;
  right: 5px;
  top: 20rpx;
  /* margin: auto 0rpx; */
}

/* 登录组件 */
.loginPop .verCodeBox {
  position: relative;
}

.loginPop .verCodeBox image {
  position: absolute;
  right: 5px;
  bottom: 5px;
  height: 70rpx;
  width: 100rpx;
  z-index: 999;
}

